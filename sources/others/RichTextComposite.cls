!RichTextComposite methodsFor!

beReadOnly

	richTextPresenter view isReadOnly: true!

caretPosition

	^richTextPresenter view caretPosition!

createComponents

	super createComponents.

	richTextPresenter := self add: RichTextPresenter new name: 'richText'.
	textoBuscarPresenter := self add: TextPresenter new name: 'textoABuscar'.!

find: aFindDetails range: anIntervalOfInteger 

	^richTextPresenter view find: aFindDetails range: anIntervalOfInteger!

findText
	| inicio intervalo |

	inicio := self caretPosition.
	"(inicio = 1) ifFalse: [inicio := inicio + textoBuscarPresenter value size]."
	intervalo := self find: (FindDetails newPattern:  textoBuscarPresenter value) range: (inicio to:  richTextPresenter value asString size).
	richTextPresenter selectionRange: intervalo.!

model

	^richTextPresenter model!

model: aModel

	super model: aModel.

	richTextPresenter model: aModel.

	self richTextCompositeZoom isZero ifTrue: [^aModel].

	self richTextCompositeZoom negative ifTrue: [^1 to: self richTextCompositeZoom abs do: [:index | self zoomOutRichText]].

	1 to: self richTextCompositeZoom do: [:index | self zoomInRichText]!

richTextCompositeZoom

	^self class richTextCompositeZoom!

selectionRange: anIntervalOfInteger 

	^richTextPresenter selectionRange: anIntervalOfInteger!

textSize

	^richTextPresenter view value size!

value

	^richTextPresenter value!

value: value

	^richTextPresenter value: value!

zoomInRichText
	"This process emulates a Mouse Wheeled event"

	richTextPresenter view defaultWindowProcessing: 522 wParam: 7864328 lParam: 50529048.!

zoomOutRichText
	"This process emulates a Mouse Wheeled event"

	richTextPresenter view defaultWindowProcessing: 522 wParam: 4287102984 lParam: 41485074.!

zoomRichText
	| presenter |

	presenter := RichTextShell show.
	presenter model: self model.

	presenter view extent: 659@600.! !
