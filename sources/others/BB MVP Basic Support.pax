!Date methodsFor!

printShortFormat
	"
Date today printUMLShortFormat. Date fromString: '20/04/2010' format: 'dd/MM/yyyy'.
"
	| str |

	str := String writeStream.
	self printOn: str format:'dd/MM/yyyy'.
	^str contents asString! !
